{
	"name": "spacedleet",
	"version": "2.0.2",
	"description": "Spaced repetition for LeetCode",
	"scripts": {
		"build:tailwind": "cross-env NODE_ENV=production postcss src/css/tailwind.css -o src/css/tailwind.min.css",
		"build:js": "cross-env NODE_ENV=production webpack",
		"build": "yarn build:tailwind && yarn build:js",
		"watch:tailwind": "postcss src/css/tailwind.css -o src/css/tailwind.min.css -w",
		"watch:js": "webpack-dev-server --content-base build",
		"dev": "run-p watch:*"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^1.3.0",
		"clean-webpack-plugin": "^1.0.0",
		"copy-webpack-plugin": "^4.6.0",
		"cross-env": "^6.0.3",
		"css-loader": "2.1.0",
		"file-loader": "3.0.1",
		"fs-extra": "7.0.1",
		"html-loader": "0.5.5",
		"html-webpack-plugin": "3.2.0",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.3",
		"style-loader": "^0.23.1",
		"svelte": "^3.5.1",
		"svelte-loader": "^2.13.4",
		"tailwindcss": "^1.1.4",
		"webpack": "4.33.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "3.7.1",
		"write-file-webpack-plugin": "4.5.0",
		"zip-webpack-plugin": "^3.0.0"
	},
	"dependencies": {
		"dayjs": "^1.8.17",
		"file-saver": "^2.0.2",
		"ky": "^0.16.1",
		"webextension-polyfill": "^0.5.0"
	}
}
